@model IEnumerable<Data.Models.Setting>;

@{
    ViewBag.Title = "Setting Index";
}
<h4>Settings List</h4>
<hr />
<a href="/Setting/Create" class="btn btn-primary mb-3">Create New</a>
<table id="settingTable" class="table table-striped table-bordered table-action-btns">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(m => m.Id)</th>
            <th>@Html.DisplayNameFor(m => m.Key)</th>
            <th>@Html.DisplayNameFor(m => m.Value)</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>@item.Id</td>
            <td>@item.Key</td>
            <td>@item.Value</td>
            <th>
                <a href="/Setting/Edit/@item.Id" class="btn btn-warning">Edit</a>
                <a href="/Setting/Delete/@item.Id" class="btn btn-danger">Delete</a>
            </th>
        </tr>
        }
    </tbody>
</table>