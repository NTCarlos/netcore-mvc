@model IEnumerable<Data.Models.Setting>;

@{
    ViewBag.Title = "Setting Index";
}
<h1> Setting Works!</h1>

<table id="example" class="table table-striped table-bordered">
        <thead>
            <tr>
                <th>Id</th>
                <th>Key</th>
                <th>Value</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Key</td>
                    <td>@item.Value</td>
                </tr>
            }
        </tbody>
    </table>

@section Styles{
    <link href="~/vendor/datatables/DataTables-1.10.24/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
}

@section Scripts{
    <script src="~/vendor/datatables/DataTables-1.10.24/js/jquery.dataTables.js"></script>
    <script src="~/vendor/datatables/DataTables-1.10.24/js/dataTables.bootstrap4.js"></script>
    <script>
        $(document).ready(function () {
            $('#example').DataTable();
        });
    </script>
}